	file = {Koyfman et al. - 2007 - Risks and benefits associated with novel phase 1 o.pdf:/home/david/Zotero/storage/H6QBRBRW/Koyfman et al. - 2007 - Risks and benefits associated with novel phase 1 o.pdf:application/pdf},
