	pages = {6609--6618},
	file = {Roth et al. - 2009 - SC68896, a novel small molecule proteasome inhibit.pdf:/home/david/Zotero/storage/32N635WQ/Roth et al. - 2009 - SC68896, a novel small molecule proteasome inhibit.pdf:application/pdf},
