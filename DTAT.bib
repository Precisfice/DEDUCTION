	author = {McQuade, Jennifer L. and Posada, Liberty P. and Lecagoonporn, Srisuda and Cain, Suzanne and Bassett, Roland L. and Patel, Sapna P. and Hwu, Wen-Jen and Hwu, Patrick and Davies, Michael A. and Bedikian, Agop Y. and Amaria, Rodabe N.},
