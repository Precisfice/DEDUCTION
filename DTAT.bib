	journal = {Eur. J. Clin. Pharmacol.},
	author = {Onoue, Haruka and Yano, Ikuko and Tanaka, Atsuko and Itohara, Kotaro and Hanai, Akiko and Ishiguro, Hiroshi and Motohashi, Hideyuki and Masuda, Satohiro and Matsubara, Kazuo},
