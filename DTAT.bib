	file = {Wang et al. - 2020 - Management of a patient with mantle cell lymphoma .pdf:/home/david/Zotero/storage/8YHK322W/Wang et al. - 2020 - Management of a patient with mantle cell lymphoma .pdf:application/pdf},
}

