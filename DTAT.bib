	keywords = {READ!!!!!, continuous endpoint, late and cumulative toxicity, longitudinal, molecularly targeted agent, oncology, phase I},
	pages = {67--80},
	file = {Yin et al. - 2017 - A Bayesian dose-finding design incorporating toxic.pdf:/home/david/Zotero/storage/U67S5N7F/Yin et al. - 2017 - A Bayesian dose-finding design incorporating toxic.pdf:application/pdf},
