	keywords = {Humans, Multiple Myeloma, Pyrroles, Glioma, Proteasome Inhibitors, Lactones, Proteasome Endopeptidase Complex, marizomib, Caspases, Chymotrypsin, Enzyme Activation, pan-proteasome inhibitor, proteasome hyperactivation, Trypsin},
	pages = {711--720},
	file = {Levin et al. - 2016 - Marizomib irreversibly inhibits proteasome to over.pdf:/home/david/Zotero/storage/K66AJCBY/Levin et al. - 2016 - Marizomib irreversibly inhibits proteasome to over.pdf:application/pdf},
