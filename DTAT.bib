	author = {Reusch, Uwe and Duell, Johannes and Ellwanger, Kristina and Herbrecht, Carmen and Knackmuss, Stefan Hj and Fucek, Ivica and Eser, Markus and McAleese, Fionnuala and Molkenthin, Vera and Gall, Fabrice Le and Topp, Max and Little, Melvyn and Zhukovsky, Eugene A.},
