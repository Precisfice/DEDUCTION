	pmid = {27859479},
	keywords = {Humans, Antineoplastic Agents, Antibodies, Monoclonal, Antibodies, Monoclonal, Humanized, Drug Eruptions, anti-CTLA-4, anti-PD-1, anti-PD-L1, dermatologic toxicities, immune checkpoint antibody, Ipilimumab},
	pages = {158--176},
	file = {Curry et al. - 2017 - Diverse types of dermatologic toxicities from immu.pdf:/home/david/Zotero/storage/7XHP38TF/Curry et al. - 2017 - Diverse types of dermatologic toxicities from immu.pdf:application/pdf},
