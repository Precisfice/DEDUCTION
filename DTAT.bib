	file = {Gonçalves et al. - 2003 - Phase I and Pharmacokinetic Study of Escalating Do.pdf:/home/david/Zotero/storage/Q5XZHHEV/Gonçalves et al. - 2003 - Phase I and Pharmacokinetic Study of Escalating Do.pdf:application/pdf},
