	pages = {173},
	file = {Kimmelman - 2012 - A theoretical framework for early human studies u.pdf:/home/david/Zotero/storage/CQYFXU58/Kimmelman - 2012 - A theoretical framework for early human studies u.pdf:application/pdf},
}

