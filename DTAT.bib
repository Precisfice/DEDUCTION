	pmid = {2743708},
	keywords = {Humans, Dose-Response Relationship, Drug, Research Design, CITE, Analysis of Variance, Hypertension, Antihypertensive Agents},
	pages = {63--77},
	file = {Sheiner et al. - 1989 - Study designs for dose-ranging.pdf:/home/david/Zotero/storage/R55R2BB2/Sheiner et al. - 1989 - Study designs for dose-ranging.pdf:application/pdf},
}

