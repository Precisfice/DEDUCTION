@article{lemaire_cold_2023,
	title = {From {Cold} to {Hot}: {Changing} {Perceptions} and {Future} {Opportunities} for {Quantitative} {Systems} {Pharmacology} {Modeling} in {Cancer} {Immunotherapy}},
