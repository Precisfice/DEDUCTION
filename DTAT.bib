	pages = {1624--1632},
	file = {Basch et al. - 2009 - Adverse symptom event reporting by patients vs cli.pdf:/home/david/Zotero/storage/PS4GMW3D/Basch et al. - 2009 - Adverse symptom event reporting by patients vs cli.pdf:application/pdf},
