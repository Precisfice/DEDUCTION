	author = {Chua, Katherina C. and Xiong, Chenling and Ho, Carol and Mushiroda, Taisei and Jiang, Chen and Mulkey, Flora and Lai, Dongbing and Schneider, Bryan P. and Rashkin, Sara R. and Witte, John S. and Friedman, Paula N. and Ratain, Mark J. and McLeod, Howard L. and Rugo, Hope S. and Shulman, Lawrence N. and Kubo, Michiaki and Owzar, Kouros and Kroetz, Deanna L.},
