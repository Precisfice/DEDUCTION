	pmid = {33010231},
	pmcid = {PMC7854276},
	keywords = {A3B1, ALL, ARI-0001, CART-cells, CD19, NHL},
	pages = {636--644},
	file = {Ortíz-Maldonado et al. - 2021 - CART19-BE-01 A Multicenter Trial of ARI-0001 Cell.pdf:/home/david/Zotero/storage/6ZF7J7B8/Ortíz-Maldonado et al. - 2021 - CART19-BE-01 A Multicenter Trial of ARI-0001 Cell.pdf:application/pdf},
