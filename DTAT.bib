	pages = {4559--4566},
	file = {Harrison et al. - 2016 - Phase I Clinical Trial of Marizomib (NPI-0052) in .pdf:/home/david/Zotero/storage/FRQ5EXDZ/FRQ5EXDZ.pdf:application/pdf},
