	pmid = {31465251},
	pages = {JOP1900044},
	file = {Yeh et al. - 2019 - Has There Been a Shift in Use of Subacute Rehabili.pdf:/home/david/Zotero/storage/TV728T48/Yeh et al. - 2019 - Has There Been a Shift in Use of Subacute Rehabili.pdf:application/pdf},
