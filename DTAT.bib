@article{mun_tumor-treating_2017,
	title = {Tumor-{Treating} {Fields}: {A} {Fourth} {Modality} in {Cancer} {Treatment}},
	issn = {1078-0432},
	shorttitle = {Tumor-{Treating} {Fields}},
	doi = {10.1158/1078-0432.CCR-17-1117},
	abstract = {Despite major advances in therapy, cancer continues to be a leading cause of mortality. In addition, toxicities of traditional therapies pose a significant challenge to tolerability and adherence. TTFields, a noninvasive anticancer treatment modality, utilizes alternating electric fields at specific frequencies and intensities to selectively disrupt mitosis in cancerous cells. TTFields target proteins crucial to the cell cycle, leading to mitotic arrest and apoptosis. TTFields also facilitate an antitumor immune response. Clinical trials of TTFields have proven safe and efficacious in patients with glioblastoma multiforme (GBM), and are FDA approved for use in newly diagnosed and recurrent GBM. Trials in other localized solid tumors are ongoing. Clin Cancer Res; 1-10. ©2017 AACR.},
