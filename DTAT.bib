	file = {Gettinger et al. - 2018 - Five-Year Follow-Up of Nivolumab in Previously Tre.pdf:/home/david/Zotero/storage/3QG84DKF/Gettinger et al. - 2018 - Five-Year Follow-Up of Nivolumab in Previously Tre.pdf:application/pdf;Snapshot:/home/david/Zotero/storage/4I2JW32W/JCO.2017.77.html:text/html},
