	year = {2018},
	pmid = {29155559},
	keywords = {Humans, Paclitaxel, Neurons, Alzheimer Disease, Peptide Fragments, Amyloid beta-Peptides, Microtubules, alanine scanning mutagenesis, Alzheimer’s disease (AD), amyloid β(1−42) fibrillation, microtubule stabilization, Neuroprotection, Neuroprotective Agents, neuroprotective peptide, primary cortical neurons, Tubulin},
	pages = {615--625},
	file = {Mondal et al. - 2018 - Crafting of Neuroprotective Octapeptide from Taxol.pdf:/home/david/Zotero/storage/KDZWUMJV/Mondal et al. - 2018 - Crafting of Neuroprotective Octapeptide from Taxol.pdf:application/pdf},
