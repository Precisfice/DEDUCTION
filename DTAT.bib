	number = {19},
	journal = {N. Engl. J. Med.},
	author = {Cortes, J. E. and Kim, D.-W. and Pinilla-Ibarz, J. and le Coutre, P. and Paquette, R. and Chuah, C. and Nicolini, F. E. and Apperley, J. F. and Khoury, H. J. and Talpaz, M. and DiPersio, J. and DeAngelo, D. J. and Abruzzese, E. and Rea, D. and Baccarani, M. and Müller, M. C. and Gambacorti-Passerini, C. and Wong, S. and Lustgarten, S. and Rivera, V. M. and Clackson, T. and Turner, C. D. and Haluska, F. G. and Guilhot, F. and Deininger, M. W. and Hochhaus, A. and Hughes, T. and Goldman, J. M. and Shah, N. P. and Kantarjian, H. and {PACE Investigators}},
