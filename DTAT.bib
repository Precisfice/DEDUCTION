	keywords = {Chemotherapy, hepatocellular carcinoma, response, single-nucleotide polymorphism, α-fetoprotein},
	pages = {630--640},
	file = {Lin et al. - 2014 - GALNT14 genotype, α-fetoprotein and therapeutic si.pdf:/home/david/Zotero/storage/UEQEHGZF/Lin et al. - 2014 - GALNT14 genotype, α-fetoprotein and therapeutic si.pdf:application/pdf},
