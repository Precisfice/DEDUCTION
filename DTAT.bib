	year = {2017},
	pmid = {29025771},
	pmcid = {PMC5718945},
	keywords = {Humans, Treatment Outcome, Blood-Brain Barrier, Antigens, CD19, Immunotherapy, Adoptive, Receptors, Antigen, T-Cell},
