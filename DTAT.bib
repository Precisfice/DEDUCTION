	pages = {4924--4936},
	file = {Zhang and Yuan - 2016 - A practical Bayesian design to identify the maximu.pdf:/home/david/Zotero/storage/DYU9Y8AY/Zhang and Yuan - 2016 - A practical Bayesian design to identify the maximu.pdf:application/pdf},
