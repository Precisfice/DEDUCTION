	file = {Yates and Mistry - 2023 - Skipping a pillar does not make for strong foundat.pdf:/home/david/Zotero/storage/84YUQAQV/Yates and Mistry - 2023 - Skipping a pillar does not make for strong foundat.pdf:application/pdf},
}

@article{hooijmaijers_building_2023,
	title = {Building an adaptive dose simulation framework to aid dose and schedule selection},
