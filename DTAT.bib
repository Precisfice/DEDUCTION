	file = {Ciccarella et al. - 2018 - Transforming research engaging patient advocates .pdf:/home/david/Zotero/storage/PXZBRMCW/Ciccarella et al. - 2018 - Transforming research engaging patient advocates .pdf:application/pdf;Snapshot:/home/david/Zotero/storage/8XTJEH4U/html.html:text/html},
}

