	pages = {101--109},
	file = {Berenbaum - 1990 - Direct search methods in the optimisation of cance.pdf:/home/david/Zotero/storage/T4YSRYYI/Berenbaum - 1990 - Direct search methods in the optimisation of cance.pdf:application/pdf},
