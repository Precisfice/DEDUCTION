	file = {Gao et al. - 2012 - Evidence for Therapeutic Drug Monitoring of Target.pdf:/home/david/Zotero/storage/474W67C3/Gao et al. - 2012 - Evidence for Therapeutic Drug Monitoring of Target.pdf:application/pdf;Snapshot:/home/david/Zotero/storage/SG2GEF3X/JCO.2012.43.html:text/html},
