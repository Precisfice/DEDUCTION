	pmid = {19862000},
	pmcid = {PMC2778518},
	keywords = {Adult, Female, Humans, Male, Middle Aged, Randomized Controlled Trials as Topic, Aged, Aged, 80 and over, Incidence, Prognosis, Carcinoma, Non-Small-Cell Lung, Antineoplastic Combined Chemotherapy Protocols, Lung Neoplasms, Neutropenia, Multicenter Studies as Topic, Granulocyte Colony-Stimulating Factor},
