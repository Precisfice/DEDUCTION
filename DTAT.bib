	pmid = {31345267},
	pmcid = {PMC6657373},
	keywords = {Ovarian cancer, Immune checkpoint inhibitor, PARP inhibitor, VEGF inhibition},
	pages = {197},
	file = {Zimmer et al. - 2019 - A phase I study of the PD-L1 inhibitor, durvalumab.pdf:/home/david/Zotero/storage/IRU29ZR7/Zimmer et al. - 2019 - A phase I study of the PD-L1 inhibitor, durvalumab.pdf:application/pdf},
