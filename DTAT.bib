	keywords = {Humans, Neoplasms, Computer Simulation, Research Design, Clinical Trials, Phase I as Topic, Models, Statistical, Treatment Outcome, Probability, Algorithms, Cell- and Tissue-Based Therapy, Immunotherapy, Adoptive, TTR},
