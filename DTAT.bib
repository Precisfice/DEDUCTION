	keywords = {Humans, Neoplasms, Clinical Trials as Topic, Precision Medicine, targeted therapy, TOR Serine-Threonine Kinases, READ\$\$\$\$\$, Antineoplastic Combined Chemotherapy Protocols, precision medicine, Drug Therapy, Combination, maximum tolerated dose, oncology, recommended phase 2 dose},
	pages = {11310--11320},
	file = {Liu et al. - 2016 - Dosing de novo combinations of two targeted drugs.pdf:/home/david/Zotero/storage/VJS5AAHR/Liu et al. - 2016 - Dosing de novo combinations of two targeted drugs.pdf:application/pdf},
