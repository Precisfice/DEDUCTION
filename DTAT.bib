	pages = {866--875},
	file = {Yin and Yuan - 2009 - A latent contingency table approach to dose findin.pdf:/home/david/Zotero/storage/HIBTVPTV/Yin and Yuan - 2009 - A latent contingency table approach to dose findin.pdf:application/pdf},
}

@article{paller_design_2014,
	title = {Design of {Phase} {I} {Combination} {Trials}: {Recommendations} of the {Clinical} {Trial} {Design} {Task} {Force} of the {NCI} {Investigational} {Drug} {Steering} {Committee}},
	volume = {20},
