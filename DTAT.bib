	pmcid = {PMC4121508},
	keywords = {Humans, Antineoplastic Agents, Neoplasms, Dose-Response Relationship, Drug, Clinical Trials, Phase I as Topic, Maximum Tolerated Dose, TTR},
	pages = {2505--2511},
	file = {Iasonos and O'Quigley - 2014 - Adaptive dose-finding studies a review of model-g.pdf:/home/david/Zotero/storage/9RLPRCGZ/Iasonos and O'Quigley - 2014 - Adaptive dose-finding studies a review of model-g.pdf:application/pdf},
}

