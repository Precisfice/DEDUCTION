	keywords = {Adult, Double-Blind Method, Humans, Male, Disease Progression, Cytokines, Antibodies, Monoclonal, Infusions, Intravenous, Leukopenia, Critical Care, C-Reactive Protein, Antibodies, Monoclonal, Humanized, Thrombocytopenia, Hypotension, CD28 Antigens, APACHE, Hemodynamics, Multiple Organ Failure},
	pages = {1018--1028},
	file = {Suntharalingam et al. - 2006 - Cytokine storm in a phase 1 trial of the anti-CD28.pdf:/home/david/Zotero/storage/KK37XKWF/Suntharalingam et al. - 2006 - Cytokine storm in a phase 1 trial of the anti-CD28.pdf:application/pdf},
