	keywords = {Female, Humans, Male, Middle Aged, Administration, Oral, Aged, Aged, 80 and over, Area Under Curve, Biological Availability, Stomach Neoplasms, Liver Neoplasms, Breast Neoplasms, Models, Biological, Antineoplastic Agents, Phytogenic, Cross-Over Studies, Creatinine, Etoposide, Injections, Intravenous, Lung Neoplasms, Software},
	pages = {511--519},
