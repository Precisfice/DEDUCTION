	month = mar,
	year = {2014},
	pmid = {24394343},
	keywords = {Double-Blind Method, Humans, Dose-finding, Dose-Response Relationship, Drug, Research Design, Pain, Adaptive design, Clinical Trials Data Monitoring Committees, Interim analysis, AZD1386, Benzimidazoles, Data Monitoring Committee, Osteoarthritis, TRPV Cation Channels},
	pages = {189--199},
	file = {Miller et al. - 2014 - Experiences with an adaptive design for a dose-fin.pdf:/home/david/Zotero/storage/ZGQUFWPP/Miller et al. - 2014 - Experiences with an adaptive design for a dose-fin.pdf:application/pdf},
