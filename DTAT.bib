	pages = {1073--1078},
	file = {Jadhav et al. - 2015 - A proposal for scientific framework enabling speci.pdf:/home/david/Zotero/storage/XQIDKPK7/Jadhav et al. - 2015 - A proposal for scientific framework enabling speci.pdf:application/pdf},
