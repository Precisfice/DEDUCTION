	keywords = {Bayes Theorem, Clinical Trials as Topic, READ, Medical Oncology, Probability, Forecasting, Random Allocation, Biostatistics},
	pages = {5371--5381},
	file = {Adamina et al. - 2009 - Bayesian statistics in oncology a guide for the c.pdf:/home/david/Zotero/storage/7F99C778/Adamina et al. - 2009 - Bayesian statistics in oncology a guide for the c.pdf:application/pdf},
}

