	keywords = {Humans, Dose-Response Relationship, Drug, Research Design, Clinical Trials as Topic, Treatment Outcome, Models, Theoretical},
	pages = {307--315},
	file = {Ivanova and Kim - 2009 - Dose finding for continuous and ordinal outcomes w.pdf:/home/david/Zotero/storage/QUWZEJ9X/Ivanova and Kim - 2009 - Dose finding for continuous and ordinal outcomes w.pdf:application/pdf},
