	author = {Maher, V. Ellen and Fernandes, Laura L. and Weinstock, Chana and Tang, Shenghui and Agarwal, Sundeep and Brave, Michael and Ning, Yang-Min and Singh, Harpreet and Suzman, Daniel and Xu, James and Goldberg, Kirsten B. and Sridhara, Rajeshwari and Ibrahim, Amna and Theoret, Marc and Beaver, Julia A. and Pazdur, Richard},
