	pmid = {10653884},
	keywords = {Humans, Animals, Antineoplastic Agents, Dose-Response Relationship, Drug, Research Design, Clinical Trials, Phase I as Topic, Maximum Tolerated Dose, READ\$\$},
	pages = {684--692},
	file = {Eisenhauer et al. - 2000 - Phase I clinical trial design in cancer drug devel.pdf:/home/david/Zotero/storage/2B9EDF7K/Eisenhauer et al. - 2000 - Phase I clinical trial design in cancer drug devel.pdf:application/pdf},
