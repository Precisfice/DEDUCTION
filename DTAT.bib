	keywords = {Humans, Cells, Cultured, Computational Biology, *Gene Transfer Techniques, DNA Transposable Elements/*genetics, Polymerase Chain Reaction, T-Lymphocytes/*metabolism, Transposases/genetics/metabolism},
	pages = {1803--1813},
	file = {Huang et al. - 2010 - Gene transfer efficiency and genome-wide integrati.pdf:/home/david/Zotero/storage/P6T54VFH/Huang et al. - 2010 - Gene transfer efficiency and genome-wide integrati.pdf:application/pdf},
