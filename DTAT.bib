	keywords = {Adolescent, Female, Humans, Male, Metabolic Clearance Rate, Models, Biological, Biomarkers, Child, Leukocytes, Mononuclear, Immunosuppressive Agents, Child, Preschool, Kidney Transplantation, Enzyme Inhibitors, Biotransformation, Glucuronides, IMP Dehydrogenase, Mycophenolic Acid, Preoperative Period, Prodrugs},
	pages = {309--320},
	file = {Fukuda et al. - 2011 - Inosine monophosphate dehydrogenase (IMPDH) activi.pdf:/home/david/Zotero/storage/2Q5XMV8N/Fukuda et al. - 2011 - Inosine monophosphate dehydrogenase (IMPDH) activi.pdf:application/pdf},
