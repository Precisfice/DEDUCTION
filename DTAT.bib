	keywords = {Female, Humans, Male, Maximum Tolerated Dose, Drug-Related Side Effects and Adverse Reactions, Treatment Outcome, Follow-Up Studies, Prospective Studies, Antineoplastic Combined Chemotherapy Protocols, Breast Neoplasms, Cyclophosphamide, Child, Child, Preschool, Pilot Projects, Methotrexate, Vincristine, Developing Countries, Retinoblastoma, Wilms Tumor},
	pages = {31--34},
