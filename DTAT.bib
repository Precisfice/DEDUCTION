	pmid = {16740385},
	keywords = {Adolescent, Adult, Humans, Antineoplastic Agents, Risk Factors, Maximum Tolerated Dose, Phytotherapy, Plant Preparations, Plant Proteins, Ribosome Inactivating Proteins, Type 2, Toxins, Biological},
	pages = {1362--1368},
	file = {Paoletti et al. - 2006 - Using the continual reassessment method lessons l.pdf:/home/david/Zotero/storage/CMXT2MG3/Paoletti et al. - 2006 - Using the continual reassessment method lessons l.pdf:application/pdf},
