	keywords = {Humans, Male, Cytokines, Syndrome, Treatment Outcome, Immunotherapy, Follow-Up Studies, Prospective Studies, Child, Antibodies, Monoclonal, Humanized, Antibodies, Bispecific, Down Syndrome, Immune System Diseases, Lymphohistiocytosis, Hemophagocytic, Macrophage Activation, Precursor B-Cell Lymphoblastic Leukemia-Lymphoma, Receptors, Interleukin-6, T-Lymphocytes, Cytotoxic},
