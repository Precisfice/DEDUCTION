	file = {Burris et al. - 2017 - Safety and Activity of Varlilumab, a Novel and Fir.pdf:/home/david/Zotero/storage/7KANT9CF/Burris et al. - 2017 - Safety and Activity of Varlilumab, a Novel and Fir.pdf:application/pdf;Snapshot:/home/david/Zotero/storage/JQPQZCW4/JCO.2016.70.html:text/html},
}

