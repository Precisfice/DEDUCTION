	keywords = {Humans, Animals, Research Design, Clinical Trials, Phase I as Topic, Maximum Tolerated Dose, Models, Theoretical, Clinical Trials, Phase II as Topic, Logistic Models, Molecular Targeted Therapy, Cytostatic Agents},
	pages = {640--646},
	file = {Hoering et al. - 2011 - Seamless Phase I-II Trial Design for Assessing Tox.pdf:/home/david/Zotero/storage/VTFXQKRJ/Hoering et al. - 2011 - Seamless Phase I-II Trial Design for Assessing Tox.pdf:application/pdf},
