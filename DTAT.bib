	keywords = {Adult, Female, Humans, Middle Aged, Chemotherapy, Adjuvant, Aged, Follow-Up Studies, Survival Analysis, Prognosis, Antineoplastic Combined Chemotherapy Protocols, Breast Neoplasms, Cyclophosphamide, Fluorouracil, Neutropenia, Age of Onset, Methotrexate, READ!!!},
	pages = {1837--1842},
	file = {Cameron et al. - 2003 - Moderate neutropenia with adjuvant CMF confers imp.pdf:/home/david/Zotero/storage/5382R2VT/Cameron et al. - 2003 - Moderate neutropenia with adjuvant CMF confers imp.pdf:application/pdf},
