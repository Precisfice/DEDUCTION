	author = {Kummar, Shivaani and Anderson, Larry and Hill, Kimberly and Majerova, Eva and Allen, Deborah and Horneffer, Yvonne and Ivy, S. Percy and Rubinstein, Larry and Harris, Pamela and Doroshow, James H. and Collins, Jerry M.},
