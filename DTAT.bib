	keywords = {Adult, Female, Humans, Male, Middle Aged, Dose-Response Relationship, Drug, Drug Administration Schedule, Aged, Aged, 80 and over, Treatment Outcome, Doxorubicin, Disease-Free Survival, Antineoplastic Combined Chemotherapy Protocols, Cyclophosphamide, Prednisone, Vincristine, Lymphoma, Large B-Cell, Diffuse, Antibodies, Monoclonal, Murine-Derived},
	pages = {1817--1826},
