	pages = {265--272},
	file = {Korn et al. - 2001 - Clinical trial designs for cytostatic agents are .pdf:/home/david/Zotero/storage/R4E3CEGD/Korn et al. - 2001 - Clinical trial designs for cytostatic agents are .pdf:application/pdf},
