	keywords = {Female, Humans, Antineoplastic Combined Chemotherapy Protocols, Breast Neoplasms, Docetaxel, Taxoids, Antibodies, Monoclonal, Humanized, Receptor, ErbB-2, Trastuzumab, Antineoplastic Agents, Immunological},
