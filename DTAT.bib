	keywords = {Humans, Time Factors, Antineoplastic Agents, Neoplasms, Dose-Response Relationship, Drug, Clinical Trials, Phase I as Topic, Maximum Tolerated Dose, Treatment Outcome, Disease-Free Survival, Prognosis, CRAP, Drug Delivery Systems},
	pages = {1289--1297},
	file = {Jain et al. - 2010 - Phase I oncology studies evidence that in the era.pdf:/home/david/Zotero/storage/TJZ8RNZK/Jain et al. - 2010 - Phase I oncology studies evidence that in the era.pdf:application/pdf},
