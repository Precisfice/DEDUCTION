	number = {3},
	journal = {J Gynecol Oncol},
	author = {Kim, Yun Hwan and Chung, Hyun Hoon and Kim, Jae Weon and Park, Noh-Hyun and Song, Yong-Sang and Kang, Soon-Beom},
	month = sep,
	year = {2009},
