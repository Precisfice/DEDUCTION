	pmid = {34418400},
	pmcid = {PMC8434816},
	keywords = {Adult, Double-Blind Method, Female, Humans, Male, Middle Aged, Young Adult, Dose-Response Relationship, Drug, Cross-Over Studies, Protein Kinase Inhibitors, Recurrence, Magnetic Resonance Imaging, Inflammation, Central Nervous System, Outcome Assessment, Health Care, Agammaglobulinaemia Tyrosine Kinase, Multiple Sclerosis, Relapsing-Remitting},
	pages = {729--738},
	file = {Reich et al. - 2021 - Safety and efficacy of tolebrutinib, an oral brain.pdf:/home/david/Zotero/storage/TVXIWL3Z/Reich et al. - 2021 - Safety and efficacy of tolebrutinib, an oral brain.pdf:application/pdf},
