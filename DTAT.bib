	keywords = {phase 1 trials, cancer, oncology clinical trials, patient expectations, patient motivations},
	file = {Dolly et al. - 2016 - A study of motivations and expectations of patient.pdf:/home/david/Zotero/storage/K8ZEDHDB/Dolly et al. - 2016 - A study of motivations and expectations of patient.pdf:application/pdf},
