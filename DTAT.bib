	file = {Helbig et al. - 2014 - Autologous Hematopoietic Stem Cell Transplantation.pdf:/home/david/Zotero/storage/UHB4U7PE/Helbig et al. - 2014 - Autologous Hematopoietic Stem Cell Transplantation.pdf:application/pdf},
