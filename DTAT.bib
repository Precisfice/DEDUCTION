	keywords = {Adult, Female, Humans, Middle Aged, Antineoplastic Agents, Dose-Response Relationship, Drug, Drug Administration Schedule, Aged, Aged, 80 and over, READ\$, Ovarian Neoplasms, Topotecan, Enzyme Inhibitors},
