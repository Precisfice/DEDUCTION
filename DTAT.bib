}

@article{mclaughlin_modelinformed_2023,
	title = {Model‐informed drug development of autologous {\textless}span style="font-variant:small-caps;"{\textgreater}{CAR}‐{T}{\textless}/span{\textgreater} cell therapy: {Strategies} to optimize {\textless}span style="font-variant:small-caps;"{\textgreater}{CAR}‐{T}{\textless}/span{\textgreater} cell exposure leveraging cell kinetic/dynamic modeling},
