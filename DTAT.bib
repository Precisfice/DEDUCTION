	pmid = {24302486},
	keywords = {Humans, Computer Simulation, Dose-Response Relationship, Drug, Models, Statistical, Clinical Trials, Phase II as Topic, Data Interpretation, Statistical, Uncertainty, Software, Neurodegenerative Diseases, binary data, READ!!!!!, CID FDA, count data, dose-response, MCP-Mod, parametric, time-to-event data},
	pages = {1646--1661},
	file = {Pinheiro et al. - 2014 - Model-based dose finding under model uncertainty u.pdf:/home/david/Zotero/storage/32M7GJX9/Pinheiro et al. - 2014 - Model-based dose finding under model uncertainty u.pdf:application/pdf},
}

@article{mason_developing_2017,
