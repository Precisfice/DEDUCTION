	file = {Iasonos and O'Quigley - 2013 - Design Considerations for Dose-Expansion Cohorts i.pdf:/home/david/Zotero/storage/PU24IF8N/Iasonos and O'Quigley - 2013 - Design Considerations for Dose-Expansion Cohorts i.pdf:application/pdf;Snapshot:/home/david/Zotero/storage/8INQ8PMX/JCO.2012.47.html:text/html},
