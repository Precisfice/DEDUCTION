	pages = {1655--1662},
	file = {Patel et al. - 2012 - Failure of iniparib to inhibit poly(ADP-Ribose) po.pdf:/home/david/Zotero/storage/5WGMSGNN/Patel et al. - 2012 - Failure of iniparib to inhibit poly(ADP-Ribose) po.pdf:application/pdf},
