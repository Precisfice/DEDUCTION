	number = {8},
	journal = {J. Clin. Oncol.},
	author = {McDermott, David F. and Sosman, Jeffrey A. and Sznol, Mario and Massard, Christophe and Gordon, Michael S. and Hamid, Omid and Powderly, John D. and Infante, Jeffrey R. and Fassò, Marcella and Wang, Yan V. and Zou, Wei and Hegde, Priti S. and Fine, Gregg D. and Powles, Thomas},
