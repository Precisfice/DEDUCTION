	pages = {344--350},
	file = {Tyler Watkins et al. - 2017 - Dose to mass for evaluation and optimization of lu.pdf:/home/david/Zotero/storage/ML3N25SI/Tyler Watkins et al. - 2017 - Dose to mass for evaluation and optimization of lu.pdf:application/pdf},
