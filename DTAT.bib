	month = jun,
	year = {2009},
	pmid = {19528132},
	pmcid = {PMC2884971},
	keywords = {Humans, Dose-Response Relationship, Drug, Clinical Trials as Topic, Drug-Related Side Effects and Adverse Reactions, Models, Theoretical, Algorithms, Pharmaceutical Preparations, Calibration},
	pages = {227--238},
	file = {Lee and Cheung - 2009 - Model calibration in the continual reassessment me.pdf:/home/david/Zotero/storage/872K68H8/Lee and Cheung - 2009 - Model calibration in the continual reassessment me.pdf:application/pdf},
}

