	pmid = {25858148},
	keywords = {READ\$\$\$},
	pages = {2232--2242},
	file = {Snapshot:/home/david/Zotero/storage/KEEV9GY9/2232.html:text/html;Vanpouille-Box et al. - 2015 - TGFβ Is a Master Regulator of Radiation Therapy-In.pdf:/home/david/Zotero/storage/UUYMUZ2H/Vanpouille-Box et al. - 2015 - TGFβ Is a Master Regulator of Radiation Therapy-In.pdf:application/pdf},
