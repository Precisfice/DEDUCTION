	keywords = {Multiple myeloma, Meta-analysis, Efficacy, Refractory, Relapsed, Triplets},
	pages = {249--255},
	file = {Sun et al. - 2017 - Triplet versus doublet combination regimens for th.pdf:/home/david/Zotero/storage/6KF4BSTU/Sun et al. - 2017 - Triplet versus doublet combination regimens for th.pdf:application/pdf},
