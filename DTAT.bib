	pages = {1642--1649},
	file = {Seidman et al. - 2008 - Randomized phase III trial of weekly compared with.pdf:/home/david/Zotero/storage/3R9CHWU8/Seidman et al. - 2008 - Randomized phase III trial of weekly compared with.pdf:application/pdf},
