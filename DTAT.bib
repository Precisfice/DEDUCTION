	pages = {917--924},
	file = {Han et al. - 2015 - Population pharmacokinetics and dosing implication.pdf:/home/david/Zotero/storage/T5BNR6FZ/Han et al. - 2015 - Population pharmacokinetics and dosing implication.pdf:application/pdf},
