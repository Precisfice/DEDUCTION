	journal = {Cancer Immunol Res},
	author = {Lisberg, Aaron and Tucker, D. Andrew and Goldman, Jonathan W. and Wolf, Brian and Carroll, James and Hardy, Ariana and Morris, Karolyn and Linares, Paulina and Adame, Carlos and Spiegel, Marshall L. and Wells, Courtney and McKenzie, Jordan and Ledezma, Blanca and Mendenhall, Melody and Abarca, Phillip and Bornazyan, Krikor and Hunt, Jaime and Moghadam, Nima and Chong, Natalie and Nameth, Danielle and Marx, Caitlin and Madrigal, John and Vangala, Sitaram and Shaverdian, Narek and Elashoff, David A. and Garon, Edward B.},
