	month = jun,
	year = {2016},
	pmid = {27250931},
	keywords = {Humans, Drug Approval, Antineoplastic Agents, Neoplasms, Dose-Response Relationship, Drug, Clinical Trials as Topic, READ, Protein Kinase Inhibitors, Molecular Targeted Therapy},
	pages = {2613--2617},
	file = {Jänne et al. - 2016 - Dose Finding of Small-Molecule Oncology Drugs Opt.pdf:/home/david/Zotero/storage/YFEJ5J6D/Jänne et al. - 2016 - Dose Finding of Small-Molecule Oncology Drugs Opt.pdf:application/pdf},
