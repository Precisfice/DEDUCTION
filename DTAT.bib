	file = {Storer - 1989 - Design and analysis of phase I clinical trials.pdf:/home/david/Zotero/storage/NU2DAUTZ/Storer - 1989 - Design and analysis of phase I clinical trials.pdf:application/pdf},
}

@article{garrett-mayer_continual_2006-1,
