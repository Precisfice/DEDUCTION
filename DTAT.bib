	pmcid = {PMC3618714},
	keywords = {Humans, Antineoplastic Agents, Neoplasms, Drug Administration Schedule, CITE, Drug Resistance, Neoplasm, Phenotype, Models, Biological, Markov Chains, Stochastic Processes, Gene Dosage},
	pages = {065005},
	file = {Liao et al. - 2012 - Conceptualizing a tool to optimize therapy based o.pdf:/home/david/Zotero/storage/KCAIABDV/Liao et al. - 2012 - Conceptualizing a tool to optimize therapy based o.pdf:application/pdf},
