	file = {Robat et al. - 2012 - Safety evaluation of combination vinblastine and t.pdf:/home/david/Zotero/storage/GKURJ5N3/Robat et al. - 2012 - Safety evaluation of combination vinblastine and t.pdf:application/pdf},
