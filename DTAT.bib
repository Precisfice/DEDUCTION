	pmid = {30561713},
	pmcid = {PMC6376915},
	keywords = {READ\$\$\$\$},
	pages = {118--128},
	file = {Hobbs et al. - 2019 - Seamless Designs Current Practice and Considerati.pdf:/home/david/Zotero/storage/6K45ZI53/Hobbs et al. - 2019 - Seamless Designs Current Practice and Considerati.pdf:application/pdf},
