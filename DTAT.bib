	keywords = {Humans, Antineoplastic Agents, Neoplasms, Dose-Response Relationship, Drug, Research Design, Clinical Trials, Phase I as Topic, Maximum Tolerated Dose, Models, Statistical, Treatment Outcome, Cohort Studies},
