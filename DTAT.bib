	pages = {3721--3738},
	file = {Bailey - 2009 - Designs for dose-escalation trials with quantitati.pdf:/home/david/Zotero/storage/XTSPH5M2/Bailey - 2009 - Designs for dose-escalation trials with quantitati.pdf:application/pdf},
