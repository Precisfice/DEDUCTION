	pmid = {20215546},
	keywords = {Humans, Antineoplastic Agents, Neoplasms, Research Design, Clinical Trials, Phase I as Topic, READ, Biomarkers, Tumor, United States, United States Food and Drug Administration, Practice Guidelines as Topic},
	pages = {1710--1718},
	file = {LoRusso et al. - 2010 - An overview of the optimal planning, design, and c.pdf:/home/david/Zotero/storage/MK8BM853/LoRusso et al. - 2010 - An overview of the optimal planning, design, and c.pdf:application/pdf},
