	keywords = {Humans, Antineoplastic Agents, Neoplasms, Risk Factors, Clinical Trials, Phase I as Topic, Maximum Tolerated Dose, READ\$\$\$, Drug-Related Side Effects and Adverse Reactions, Treatment Outcome, Risk Assessment, Data Interpretation, Statistical, Outcome Assessment (Health Care)},
	pages = {1555--1563},
	file = {Wages et al. - 2011 - Continual reassessment method for partial ordering.pdf:/home/david/Zotero/storage/WN9P9MH9/Wages et al. - 2011 - Continual reassessment method for partial ordering.pdf:application/pdf},
