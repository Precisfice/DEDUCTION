	pages = {4949--4959},
	file = {Karam et al. - 2018 - Final Report of a Phase I Trial of Olaparib with C.pdf:/home/david/Zotero/storage/U6TMFXG5/Karam et al. - 2018 - Final Report of a Phase I Trial of Olaparib with C.pdf:application/pdf},
