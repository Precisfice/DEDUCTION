	journal = {medRxiv},
	author = {Scott, Jacob G. and Sedor, Geoffrey and Ellsworth, Patrick and Scarborough, Jessica A. and Ahmed, Kamran and Eschrich, Steven A. and Torres-Roca, Javier F. and Kattan, Michael W.},
