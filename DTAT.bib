	keywords = {Adult, Female, Humans, Male, Middle Aged, Recombinant Proteins, Young Adult, Dose-Response Relationship, Drug, Aged, Doxorubicin, Follow-Up Studies, Disease-Free Survival, Kaplan-Meier Estimate, Retrospective Studies, Antineoplastic Combined Chemotherapy Protocols, Cyclophosphamide, Etoposide, Granulocyte Colony-Stimulating Factor, Prednisone, Vincristine, Lymphoma, Large B-Cell, Diffuse, Rituximab, Antibodies, Monoclonal, Murine-Derived, Filgrastim, Stroke Volume},
	pages = {1408--1416},
