	keywords = {Humans, Male, Animals, Time Factors, Computer Simulation, Models, Biological, Immunosuppression, Immunosuppressive Agents, Myeloid Cells, Rats, Rats, Sprague-Dawley},
	pages = {744--753},
	file = {Friberg et al. - 2010 - Scaling the time-course of myelosuppression from r.pdf:/home/david/Zotero/storage/9Z62J6H6/Friberg et al. - 2010 - Scaling the time-course of myelosuppression from r.pdf:application/pdf},
