	keywords = {Humans, Clinical Trials, Phase III as Topic, Time Factors, Antineoplastic Agents, Chemotherapy, Adjuvant, Randomized Controlled Trials as Topic, Research Design, Clinical Trials, Phase II as Topic, Patient Selection, Reproducibility of Results, Life Expectancy, Endpoint Determination, Molecular Targeted Therapy, Sample Size, Treatment Failure, Health Resources, Selection Bias, Evidence-Based Medicine, Confounding Factors, Epidemiologic},
	pages = {1093--1100},
	file = {Sharma et al. - 2011 - Randomized phase II trials a long-term investment.pdf:/home/david/Zotero/storage/F85PBA6S/Sharma et al. - 2011 - Randomized phase II trials a long-term investment.pdf:application/pdf},
