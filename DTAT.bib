	journal = {J. Natl. Cancer Inst.},
	author = {Budman, D. R. and Berry, D. A. and Cirrincione, C. T. and Henderson, I. C. and Wood, W. C. and Weiss, R. B. and Ferree, C. R. and Muss, H. B. and Green, M. R. and Norton, L. and Frei, E.},
